<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="428.0" prefWidth="644.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="socialnetwork.ui.gui.FriendRequestsController">
   <children>
      <Label contentDisplay="CENTER" layoutX="77.0" layoutY="12.0" prefHeight="28.0" prefWidth="137.0" style="-fx-alignment: CENTER;" text="Received" textAlignment="CENTER">
         <font>
            <Font name="MS Reference Sans Serif" size="22.0" />
         </font>
      </Label>
      <TableView id="receivedFriendRequestsTable" fx:id="receivedFriendRequestsTable" layoutX="14.0" layoutY="53.0" prefHeight="312.0" prefWidth="284.0">
         <columns>
            <TableColumn fx:id="firstNameFRPColumn" prefWidth="106.0" style="-fx-alignment: CENTER;" text="First name" />
            <TableColumn fx:id="lastNameFRPColumn" prefWidth="98.0" style="-fx-alignment: CENTER;" text="Last name" />
            <TableColumn fx:id="dateFRPColumn" prefWidth="78.0" style="-fx-alignment: CENTER;" text="Date" />
         </columns>
      </TableView>
      <Button fx:id="acceptButton" layoutX="35.0" layoutY="376.0" mnemonicParsing="false" onAction="#acceptFriendRequest" prefHeight="39.0" prefWidth="83.0" text="Accept" />
      <Button fx:id="declineButton" layoutX="194.0" layoutY="376.0" mnemonicParsing="false" onAction="#declineFriendRequest" prefHeight="39.0" prefWidth="83.0" text="Decline" />
      <Label contentDisplay="CENTER" layoutX="420.0" layoutY="12.0" prefHeight="28.0" prefWidth="137.0" style="-fx-alignment: CENTER;" text="Sent" textAlignment="CENTER">
         <font>
            <Font name="MS Reference Sans Serif" size="22.0" />
         </font>
      </Label>
      <TableView id="sentFriendRequestsTable" fx:id="sentFriendRequestsTable" layoutX="346.0" layoutY="52.0" prefHeight="312.0" prefWidth="284.0">
         <columns>
            <TableColumn fx:id="firstNameFRSColumn" prefWidth="106.0" style="-fx-alignment: CENTER;" text="First name" />
            <TableColumn fx:id="lastNameFRSColumn" prefWidth="98.0" style="-fx-alignment: CENTER;" text="Last name" />
            <TableColumn fx:id="dateFRSColumn" prefWidth="78.0" style="-fx-alignment: CENTER;" text="Date" />
         </columns>
      </TableView>
      <Button fx:id="cancelButton" layoutX="447.0" layoutY="376.0" mnemonicParsing="false" onAction="#cancelFriendRequest" prefHeight="39.0" prefWidth="83.0" text="Cancel" />
   </children>
</AnchorPane>
