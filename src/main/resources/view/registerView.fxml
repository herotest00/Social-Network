<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.Tooltip?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="registerPane" focusTraversable="true" prefHeight="457.0" prefWidth="644.0" stylesheets="@../css/main.css, @../css/defaultTheme.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="socialnetwork.ui.gui.RegisterController">
   <children>
      <Label id="registerLabel" alignment="CENTER" contentDisplay="CENTER" focusTraversable="true" layoutX="204.0" layoutY="14.0" prefHeight="105.0" prefWidth="233.0" text="Register form" textAlignment="CENTER" textFill="WHITE" textOverrun="CLIP" wrapText="true">
         <font>
            <Font name="Arial" size="32.0" />
         </font>
      </Label>
      <TextField fx:id="username" layoutX="247.0" layoutY="143.0" promptText="Username" />
      <PasswordField fx:id="password" layoutX="247.0" layoutY="189.0" promptText="Password" />
      <PasswordField fx:id="passwordConfirm" layoutX="247.0" layoutY="232.0" promptText="Password confirm" />
      <TextField fx:id="firstName" layoutX="247.0" layoutY="273.0" promptText="First name">
         <tooltip>
            <Tooltip fx:id="firstnameTooltip" text="Only the first letter must be capital, at least 3 letters" />
         </tooltip></TextField>
      <TextField fx:id="lastName" layoutX="247.0" layoutY="316.0" promptText="Last name">
         <tooltip>
            <Tooltip fx:id="lastnameTooltip" text="Only the first letter must be capital, at least 3 letters" />
         </tooltip></TextField>
      <Button fx:id="registerButton" defaultButton="true" layoutX="264.0" layoutY="358.0" mnemonicParsing="false" onAction="#registerButtonClicked" prefHeight="31.0" prefWidth="113.0" text="Register" textAlignment="CENTER" textFill="WHITE" wrapText="true">
         <font>
            <Font name="Corbel" size="12.0" />
         </font>
      </Button>
      <Button fx:id="backButton" layoutX="264.0" layoutY="403.0" mnemonicParsing="false" onAction="#backButtonClicked" prefHeight="31.0" prefWidth="113.0" text="Back" textAlignment="CENTER" textFill="WHITE" wrapText="true">
         <font>
            <Font name="Corbel" size="12.0" />
         </font>
      </Button>
      <Label fx:id="notificationLabel" />
   </children>
</AnchorPane>
